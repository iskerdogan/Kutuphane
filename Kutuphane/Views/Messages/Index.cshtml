@using Kutuphane.Models.Entity
@model List<Messages>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/Membersdashboard.cshtml";
}

<h2>Mesajlar</h2>

<table class="table table-bordered">
    <tr>
        <th>Gönderen</th>
        <th>Konu</th>
        <th>Mesaj</th>
        <th>Tarih</th>
    </tr>
    @foreach (Messages result in Model)
    {
        <tr>
            <td>@result.PostedByMember</td>
            <td>@result.Subject</td>
            <td>@result.MessageContent</td>
            <td>@result.Date</td>
        </tr>
 
    }
</table>
