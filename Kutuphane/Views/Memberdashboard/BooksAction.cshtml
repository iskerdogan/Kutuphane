@using Kutuphane.Models.Entity
@model List<Transactions>
@{
    ViewBag.Title = "BooksAction";
    Layout = "~/Views/Shared/Membersdashboard.cshtml";
    string memberReturn= " ";
    string icon = " ";
    string line = " ";
    string btnColor = " ";
    string bgColor = " ";
    string arrowColor = " ";

}

<!DOCTYPE html>
<link href="~/memberDashboard/src/css/adminlte.css" rel="stylesheet" />
<html>

<body class="hold-transition sidebar-mini">

    <section class="content">

        <div class="container-fluid">

            <div class="row">



                <!-- /.col -->

                <div class="col-md-12">

                    <div class="card">

                        <div class="card-header p-2">

                            <h2 style="text-align:center">Kitap Hareketlerim</h2>

                        </div><!-- /.card-header -->

                        <div class="card-body">



                            <div class="tab-pane" id="timeline">

                                <!-- The timeline -->

                                <div class="timeline timeline-inverse">

                                    <!-- timeline time label -->

                                    @foreach (Transactions result in Model)

                                    {
                                        if (@Convert.ToDateTime(@result.MemberReturnDate).ToString("dd/MM/yyyy") == "01.01.0001")
                                        {
                                            memberReturn = "Teslim Edilmedi";
                                            icon = "data:image/png;base64,";
                                            line = "data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAACgAAAAoCAYAAACM/rhtAAAABmJLR0QA/wD/AP+gvaeTAAAARklEQVRYhe3OQQ2AMBAAwT1cYKUBB8hFQmsFGbT/pl9yCdlRMCBJkiTpxwLguY4WULIzk7rf9dwAAt7szULPDkiSJEn63AASrwcCMMZqagAAAABJRU5ErkJggg==";
                                            btnColor = "btn btn-danger";
                                            bgColor = "bg-danger";
                                            arrowColor = "data:image/png;base64,";
                                        }
                                        else
                                        {
                                            memberReturn = @Convert.ToDateTime(@result.MemberReturnDate).ToString("dd/MM/yyyy");
                                            icon = "https://img.icons8.com/fluent/30/000000/pass.png";
                                            line = "data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAACgAAAAoCAYAAACM/rhtAAAABmJLR0QA/wD/AP+gvaeTAAAASElEQVRYhe3OsQ1AUBhG0fsspFFJDPU2MRSlxkA+tajlT+SeCQ5IkiRJ+rEGMB59T5irM0/ZzmldBoAkV3XnraV6IEmSJOlzN4mLDP/0SXZXAAAAAElFTkSuQmCC";
                                            btnColor = "btn btn-success";
                                            bgColor = "bg-success";
                                            arrowColor = "data:image/png;base64,";
                                        }

                                    <div class="time-label">

                                        <a class="btn btn-warning">

                                            <span class="bg-warning">

                                                @Convert.ToDateTime(@result.PurchaseDate).ToString("dd/MM/yyyy")

                                            </span>

                                        </a>
                                        <img src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAACgAAAAoCAYAAACM/rhtAAAABmJLR0QA/wD/AP+gvaeTAAAARUlEQVRYhe3OwQmAQAwAwY0NaD2i1XsWpAVcfB/4lYDsVDAgSZIk6ccC4DqWk8i1OjPKNm/3PgEQvRdv3mR1QJIkSdLnHn9GCgD+w8sbAAAAAElFTkSuQmCC">
                                        <img src="data:image/png;base64,">
                                        <img src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAACgAAAAoCAYAAACM/rhtAAAABmJLR0QA/wD/AP+gvaeTAAAARUlEQVRYhe3OwQmAQAwAwY0NaD2i1XsWpAVcfB/4lYDsVDAgSZIk6ccC4DqWk8i1OjPKNm/3PgEQvRdv3mR1QJIkSdLnHn9GCgD+w8sbAAAAAElFTkSuQmCC">
                                        <a class="@btnColor">

                                            <span class="@bgColor">

                                                @memberReturn

                                            </span>

                                        </a>
                                        <img src="@line">
                                        <img src="@arrowColor">
                                        <img src="@line">
                                        <img src="@icon" />
                                    </div>

                                        <!-- /.timeline-label -->

                                        <!-- timeline item -->

                                        <div>

                                            @*<i class="fas fa-comment bg-primary"></i>*@

                                            <div class="timeline-item">

                                                <div class="timeline-body">

                                                    @result.Books.Name - @result.Books.Writers.Name @result.Books.Writers.Surname

                                                </div>

                                            </div>

                                        </div>

                                    }

                                </div>

                            </div>

                        </div>

                    </div><!-- /.card-body -->

                </div>

                <!-- /.nav-tabs-custom -->

            </div>

            <!-- /.col -->

        </div>

    </section>

</body>

</html>

